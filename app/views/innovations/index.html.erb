<p id="notice"><%= notice %></p>

<h1>Innovations</h1>

<table>
  <thead>
    <tr>
      <th>Innovationid</th>
      <th>Innovationname</th>
      <th>Innovationcategory</th>
      <th>Isinnovationfavorite?</th>
      <th>Innovationtargeturl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @innovations.each do |innovation| %>
      <tr>
        <td><%= innovation.InnovationID %></td>
        <td><%= innovation.InnovationName %></td>
        <td><%= innovation.InnovationCategory %></td>
        <td><%= innovation.IsInnovationFavorite? %></td>
        <td><%= innovation.InnovationTargetUrl %></td>
        <td><%= link_to 'Show', innovation %></td>
        <td><%= link_to 'Edit', edit_innovation_path(innovation) %></td>
        <td><%= link_to 'Destroy', innovation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Innovation', new_innovation_path %>
